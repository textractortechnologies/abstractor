- if versions.any?
  %b
    History:
  = link_to '(Show)', '', class: 'show_hide_abstractor_history_link show_abstractor_history'
  .abstactor_history_content.hide
    - versions.each do |version|
      .version
        .version_date
          %i
            Date:
          = version.created_at.to_date.to_s(:date)
        .version_value
          %i
            Value:
          = version.reify.display_value
        .version_user
          %i
            User:
          = version.originator